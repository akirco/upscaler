{
  "name": "upscaler",
  "author": "aki",
  "description": "upscaler",
  "email": "earlfx@163.com",
  "version": "1.0.5",
  "license": "ISC",
  "main": "dist/.electron/main/main.js",
  "scripts": {
    "dev": "kill-port 3000 && concurrently -k \"vite\" \"wait-on tcp:3000 && npm-run-all watch\"",
    "watch": "tsc-watch --noClear -p tsconfig.electron.json --onSuccess \"npm-run-all start\"",
    "start": "cross-env NODE_ENV=development electron ./dist/.electron/main/main.js",
    "render:dev": "vite",
    "main:dev": "run-electron .",
    "clear": "rimraf dist build images",
    "build": "npm-run-all clear build:vue build:tsc build:all",
    "build:vue": "vue-tsc --noEmit && vite build",
    "build:tsc": "tsc -p tsconfig.electron.json",
    "build:all": "electron-builder",
    "release": "yarn run build && electron-builder -w --publish always",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vue-loading-overlay": "^6.0.3"
  },
  "devDependencies": {
    "@babel/types": "^7.21.2",
    "@headlessui/vue": "^1.7.11",
    "@heroicons/vue": "^2.0.16",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "daisyui": "^2.51.1",
    "dotenv": "^16.0.3",
    "electron": "23.1.1",
    "electron-builder": "^23.6.0",
    "electron-devtools-vendor": "^1.2.0",
    "kill-port": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "run-electron": "^1.0.0",
    "tailwindcss": "^3.2.7",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0",
    "wait-on": "^7.0.1"
  }
}
